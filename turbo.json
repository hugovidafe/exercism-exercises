{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": ["yarn.lock"],
	"pipeline": {
		"test": {
			"inputs": [
				"*.ts",
				"babel.config.cjs",
				"jest.config.cjs",
				"tsconfig.json"
			],
			"outputs": []
		},
		"lint": {
			"inputs": [
				"*.ts",
				".eslintignore",
				".eslintrc.cjs",
				"tsconfig.json"
			],
			"outputs": []
		}
	}
}
